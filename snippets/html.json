	},
	"amexio-layout-columns": {
		"prefix": "amexio-layout-columns",
		"body": [
			"<amexio-layout-columns",
			"\t[border]=\"${1:true}\"",
			"\t[fit]=\"${2:true}\"",
			"\t[alignment]=\"'${3:alignment}'\"",
			"\t[orientation]=\"'${4:vertical}'\"",
			"\t[responsive-mode]=\"'${5:false}'\">",
			"\t<amexio-layout-item",
			"\t\t[fit]=\"${6:true}\"",
			"\t[padding]=\"'${7:padding}'\"",
			"\t\t*ngFor=\"let alignment of ['start','space-between','center']\">",
			"\t</amexio-layout-item>",
			"</amexio-layout-columns>"
		],
		"description": "Amexio Layout Columns"
	},
	"amexio-layout-item": {
		"prefix": "amexio-layout-item",
		"body": [
			"<amexio-layout-item",
			"\t*ngFor=\"let alignment of ['start','space-between','center']\"",
			"\t[fit]=\"${1:true}\"",
			"\t[padding]=\"'${2:padding}'\">",
			"</amexio-layout-item>"
		],
		"description": "Amexio Layout Item"
	},
	"amexio-layout-grid": {
		"prefix": "amexio-layout-grid",
		"body": [
			"<amexio-layout-grid",
			"\t\t[layout]=\"'${1:layout}'\">",
			"\t<amexio-grid-item",
			"\t\t[name]=\"'${1:name}'\">",
			"\t</amexio-grid-item>",
			"</amexio-layout-grid>"
		],
		"description": "Amexio Layout Grid"
	},
	"amexio-grid-item": {
		"prefix": "amexio-grid-item",
		"body": [
			"<amexio-grid-item",
			"\t[name]=\"'${1:name}'\">",
			"</amexio-grid-item>"
		],
		"description": "Amexio Grid Item"
	},
	"amexio-card-ce": {
		"prefix": "amexio-card-ce",
		"body": [
			"<amexio-card-ce",
			"align=\"${1:start}\"",
			"bg-image=\"${2:bg-image}\"",
			"color=\"${3:color}\"",
			"background=\"${4:background}\"",
			"height=\"${5:height}\"",
			"style-type=\"${6:style-type}\"",
			"enable-lifecycle-events=\"${7:lifecycle-events}\">",
			"<amexio-header-ce", 
			"\t\t[background]=\"'${8:background}'\"", 
			"\t\t[height]=\"'${9:height}'\"",
			"\t\talign=\"${10:start}\">",
            "</amexio-header-ce>",
			"<amexio-body-ce", 
			"\t\t[align]=\"'${11:start}'\">",
            "</amexio-body-ce>",
			"<amexio-action-ce", 
			"\t\t[align]=\"'${12:start}'\"",
			 "\t\t[border-top]=\"${13:false}\">",
            "</amexio-action-ce>",
            "</amexio-card-ce>"
		],
		"description": "Creative card"
	},
	"amexio-homepage-ce": {
		"prefix": "amexio-homepage-ce",
		"body": [
			"<amexio-homepage-ce type=\"${1:1}\">",
			"\t\t<amexio-homepage-northpanel>",
            "\t\t</amexio-homepage-northpanel>",
			"\t\t<amexio-homepage-westpanel>", 
            "\t\t</amexio-homepage-westpanel>",
			"\t\t<amexio-homepage-centerpanel>", 
            "\t\t</amexio-homepage-centerpanel>",
            "</amexio-homepage-ce>"
		],
		"description": "Creative card"
	},
	//Panel
	//Panel-carousel
	"amexio-carousel": {
		"prefix": "amexio-carousel",
		"body": [
			"<amexio-carousel [header]=\"'${1:Header}'\" [data]=\"${2:data}\">",
			"\t<ng-template let-img amexioTemplate=\"${3:item}\">",
			"\t\t${4:content}",
			"\t</ng-template>",
			"</amexio-carousel>"
		],
		"description": "Amexio Carousel"
	},
	"amexio-carousel-shuffle": {
		"prefix": "amexio-carousel-shuffle",
		"body": [
			"<amexio-carousel [header]=\"'${1:header}'\" [data]=\"${2:data}\" shuffle-interval=\"${3:interval}\">",
			"\t<ng-template let-img amexioTemplate=\"${4:item}\">",
			"\t\t${5:content}",
			"\t</ng-template>",
			"</amexio-carousel>"
		],
		"description": "Amexio Carousel with Shuffle Interval"
	},
	//Panels-Form
	"amexio-form": {
		"prefix": "amexio-form",
		"body": [
			"<amexio-form",
			"\t[header]=\"${1:false}\"",
			"\t[header-align]=\"'${2:left}'\"",
			"\t[footer-align]=\"'${3:right}'\"",
			"\t[min-height]=\"${4:height}\"",
			"\t[body-height]=\"'${5:height}'\"",
			"\t[form-name]=\"${6:name}\"",
			"\t[show-error]=\"${7:false}\"",
			"\t[enable-lifecycle-events]=\"${8:events}\"",
			"\t(showErrorMsg)=\"${9:functionName}(\\$event)\"",
			"\t(initiated)=\"${10:functionName}(\\$event)\"",
			"\t(ready)=\"${11:functionName}(\\$event)\"",
			"\t(destroy)=\"${12:functionName}(\\$event)\">",
			"\t\t<amexio-form-header>",
			"\t\t\t${13:header}",
			"\t\t</amexio-form-header>",
			"\t\t<amexio-form-body>",
			"\t\t\t${14:body}",
			"\t\t</amexio-form-body>",
			"\t\t<amexio-form-action>",
			"\t\t\t${15:footer}",
			"\t\t</amexio-form-action>",
			"</amexio-form>"
		],
		"description": "Amexio Form "
	},
	"amexio-form-ce": {
		"prefix": "<amexio-form-ce>",
		"body": [
			"<amexio-form-ce", 
			"\tcolor=\"${1:white}\"",
			"\tform-name=\"${2:name}\"", 
			"\t[show-error]=\"${3:false}\"",
			"\talign=\"${4:start}\"",
			"\t[enable-lifecycle-events]=\"${5:events}\"",
			"\t(initiated)=\"${6:functionName}(\\$event)\"",
			"\t(ready)=\"${7:functionName}(\\$event)\"",
			"\t(destroy)=\"${8:functionName}(\\$event)\">",
			"\t\t<amexio-header-ce",
			"\t\t\t[background]=\"'${9:background}'\"", 
			"\t\t\theight=\"${10:height}\"", 
			"\t\t\talign=\"${11:start}\"",
			"\t\t\tborder-bottom=\"${12:false}\"",
			"\t\t\tdirection=\"${13:row}\">", 
            "\t\t</amexio-header-ce>",
			"\t\t<amexio-body-ce",
			"\t\t\theight=\"${14:height}\"", 
			"\t\t\talign=\"${15:end}\">", 
            "\t\t</amexio-body-ce>",
			"\t\t<amexio-action-ce",
			"\t\t\t[align]=\"'${16:end}'\"", 
			"\t\t\t[border-top]=\"${17:false}\"",
			"\t\t\tdirection=\"${18:row}\">", 
            "\t\t</amexio-action-ce>",
            "</amexio-form-ce>"
		],
		"description": "Creative Window"
	},
	//Panels-fieldset
	"amexio-fieldset": {
		"prefix": "amexio-fieldset",
		"body": [
			"<amexio-fieldset [collapsible]=\"${1:false}\" [title]=\"${2:title}\">",
			"</amexio-fieldset>"
		],
		"description": "Amexio Fieldset"
	},
	"amexio-fieldset-collapsed": {
		"prefix": "amexio-fieldset-collapsed",
		"body": [
			"<amexio-fieldset [collapsible]=\"true\" [title]=\"${1:title}\">",
			"</amexio-fieldset>"
		],
		"description": "Amexio Fieldset "
	},
	//Panels-panel
	"amexio-panel": {
		"prefix": "amexio-panel",
		"body": [
			"<amexio-panel",
			"\t[header]=\"${1:true}\"",
			"\t[title]=\"'${2:title}'\"",
			"\t[height]=\"${3:height}\"",
			"\t[expanded]=\"${4:false}\"",
			"\t[border]=\"${5:false}\"",
			"\t[content-align]=\"${6:left}\"",
			"\t[collapsible]=\"${7:true}\"",
			"\t[context-menu]=\"${8:true}\"",
			"\t[background]=\"${10:background}\"",
			"\t[color]=\"${11:color}\"",
			"\t[enable-lifecycle-events]=\"${12:lifecycle-events}\"",
			"\t(onClick)=\"${13:functionName}(\\$event)\"",
			"\t(rightClick)=\"${14:functionName}(\\$event)\"",
			"\t(initiated)=\"${15:functionName}(\\$event)\"",
			"\t(ready)=\"${16:functionName}(\\$event)\"",
			"\t(destroy)=\"${17:functionName}(\\$event)\">",
			"<amexio-panel-header>",
			"\t\t${9:content}",
			"</amexio-panel-header>",
			"</amexio-panel>"
		],
		"description": "Amexio Panel"
	},
	"amexio-panel-context-menu": {
		"prefix": "amexio-panel-context-menu",
		"body": [
			"<amexio-panel",
			"\t[header]=\"${1:true}\"",
			"\t[title]=\"'${2:title}'\"",
			"\t[height]=\"${3:height}\"",
			"\t[expanded]=\"${4:true}\"",
			"\t[border]=\"${5:border}\"",
			"\t[content-align]=\"${6:left}\"",
			"\t[collapsible]=\"${7:true}\"",
			"\t[context-menu]=\"${8:context-menu}\"",
			"\t(onClick)=\"${9:functionName}(\\$event)\">",
			"\t\t${10:content}",
			"</amexio-panel>"
		],
