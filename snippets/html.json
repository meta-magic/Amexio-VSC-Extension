			"\t[header]=\"${1:true}\"",
			"\t[footer]=\"${3:false}\"",
			"\t[footer-align]=\"'${4:right}'\"",
			"\t[context-menu]=\"'${5:rightclick}'\"",
			"\t[show]=\"'${6:true}'\"",
			"\t[height]=\"${7:height}\"",
			"\t[enable-lifecycle-events]=\"${8:lifecycle-events}\"",
			"\t(rightClick)=\"${9:functionName}(\\$event)\"",
			"\t(initiated)=\"${10:functionName}(\\$event)\"",
			"\t(ready)=\"${11:functionName}(\\$event)\"",
			"\t(destroy)=\"${12:functionName}(\\$event)\">",
			"\t\t<amexio-header>",
			"\t\t\t${13:Header}",
			"\t\t</amexio-header>",
			"\t\t<amexio-body>",
			"\t\t\t${14:body}",
			"\t\t</amexio-body>",
			"\t\t<amexio-action>",
			"\t\t\t${15:footer}",
			"\t\t</amexio-action>",
			"</amexio-card>"
		],
		"description": "Amexio Card with Context-Menu"
	},
	"amexio-card-with-flag": {
