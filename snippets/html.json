  //Text Area
  "amexio-textarea": {
    "prefix": "amexio-textarea-input",
    "body": [
      "<amexio-textarea-input [fieldLabel]=\"'${1:label}'\" name =\"${2:name}\"",
      "\t [placeholder]=\"'${3:placeHolder}'\"",
      "\t [allowBlank]=\"true\"",
      "\t [errorMsg] =\"'${4:errorMsg}'\"",
      "\t [iconFeedBack]=\"false\"",
      "\t [noOfrows]=\"${5:noOfrows}\"",
      "\t [noOfCols]=\"${6:noOfCols}\"",
      "\t [popoverPlacement]=\"'${7:popoverPlacement}'\">",
      "</amexio-textarea-input>"
    ],
    "description": "Amexio TextArea Input"
  },
  "amexio-textarea-input-with-columns-rows": {
    "prefix": "amexio-textarea-input-with-columns-rows",
    "body": [
      "<amexio-textarea-input [fieldLabel]=\"'${1:label}'\" name =\"${2:name}\"",
      "\t [placeholder]=\"'${3:placeHolder}'\"",
      "\t [allowBlank]=\"true\"",
      "\t [errorMsg] =\"'${4:errorMsg}'\"",
      "\t [iconFeedBack]=\"false\"",
      "\t [noOfrows]=\"${5:noOfrows}\"",
      "\t [noOfCols]=\"${6:noOfCols}\">",
      "</amexio-textarea-input>"
    ],
    "description": "Amexio TextArea Input with Rows and Columns"
  },
  //Tab Pane
  "amexio-tab": {
    "prefix": "amexio-tab",
    "body": [
      "<amexio-tab-pane  [closable]=\"false\">",
      "\t<amexio-tab title=\"'${1:title}'\" [active]=\"true\">",
      "\t${2:Html}",
      "\t\"</amexio-tab>\"",
      "\t<amexio-tab title=\"'${3:title2}'\">",
      "\t${4:Html2}",
      "\t\"</amexio-tab>\"",
      "\t<amexio-tab title=\"'${5:title3}'\">",
      "\t${6:Html3}",
      "\t\"</amexio-tab>\"",
      "\t<amexio-tab title=\"'${7:title4}'\">",
      "\t${8:Html4}",
      "\t\"</amexio-tab>\""
    ],
    "description": "Amexio Simple Tab Pane "
  },
  "amexio-tab-scrollable": {
    "prefix": "amexio-tab-scrollable",
    "body": [
      "<amexio-tab-pane  [closable]=\"false\">",
      "\t<amexio-tab title=\"'${2:title}'\" [active]=\"true\">",
      "\t${3:Html}",
      "\t\"</amexio-tab>\"",
      "\t<amexio-tab title=\"'${4:title2}'\">",
      "\t${5:Html2}",
      "\t\"</amexio-tab>\"",
      "\t<amexio-tab title=\"'${6:title3}'\">",
      "\t${10:Html3}",
      "\t\"</amexio-tab>\"",
      "\t<amexio-tab title=\"'${11:title4}'\">",
      "\t${13:Html4}",
      "\t\"</amexio-tab>\""
    ],
    "description": "Amexio Tab Scrollable"
  },
  "amexio-tab-with-icons": {
    "prefix": "amexio-tab-with-icons",
    "body": [
      "<amexio-tab-pane [icons]=\"true\">",
      "\t <amexio-tab title=\"${1:title}\" [active]=\"true\"",
      "\t [icon]=\"'${2:icon}'\">",
      "\t ${3:HTML}</amexio-tab>",
      "\t <amexio-tab title=\"${4:title2}\"",
      "\t [icon]=\"'${5:icon2}'\">",
      "\t ${6:HTML2}</amexio-tab>",
      "\t </amexio-tab>",
      "\t <amexio-tab title=\"${7:title3}\"",
      "\t [icon]=\"'${8:icon3}'\">",
      "\t ${9:HTML3}</amexio-tab>",
      "\t </amexio-tab>",
      "\t </amexio-tab-pane>"
    ],
    "description": "Amexio Tab with icons"
  },
  //CheckBox Group Snippet
  "amexio-check-box": {
    "prefix": "amexio-horizontal-check-box",
    "body": [
      "<amexio-checkbox [fieldLabel]=\"'Horizontal Checkbox'\" name =\"${1:name}\"",
      "\t [dataReader]=\"'response.data'\"",
      "\t [displayField]=\"'${2:displayField}'\"",
      "\t [valueField]=\"'${3:valueField}'\"",
      "\t [checkBoxGroupDownBindData]=\"checkboxGroupdata\"",
      "\t [column]=\"'${4:columnCount}'\">",
      "</amexio-checkbox>"
    ],
    "description": "Amexio Horizontal Checkbox Group"
  },
  "amexio-check-box-vertical": {
    "prefix": "amexio-vertical-check-box",
    "body": [
      "<amexio-checkbox [fieldLabel]=\"'Vertical Checkbox'\" name =\"'${1:name}'\"",
      "\t [dataReader]=\"'response.data'\"",
      "\t [displayField]=\"'${2:displayField}'\"",
      "\t [valueField]=\"'${3:valueField}'\"",
      "\t [checkBoxGroupDownBindData]=\"checkboxGroupdata\"",
      "\t [column]=\"'${4:columnCount}'\">",
      "</amexio-checkbox>"
    ],
    "description": "Amexio Vertical Checkbox Group"
  },
  //Radio Group Snippet
  "amexio-radio-group": {
    "prefix": "amexio-radio-group",
    "body": [
      " <amexio-radio-group [fieldLabel]=\"'Horizontal RadioGroup'\" name =\"${1:name}\"",
      "\t [fieldName]=\"'${2:fieldName}'\"",
      "\t [dataReader]=\"'${3:dataReader}'\"",
      "\t [displayField]=\"'${4:displayField}'\"",
      "\t [valueField]=\"'${5:valueField}'\"",
      "\t [radioGroupBindData]=\"radioGroupData\"",
      "\t (selectedValue)=\"setSelectedGender($${6:event})\"",
      "\t [column]=\"'${7:column}'\">",
      " </amexio-radio-group>"
    ],
    "description": "Amexio Horizontal Radio Group"
  },
  "amexio-radio-group-vertical": {
    "prefix": "amexio-radio-group-vertical",
    "body": [
      " <amexio-radio-group [fieldLabel]=\"'Vertical RadioGroup'\" name =\"${1:name}\"",
      "\t [fieldName]=\"'${2:fieldName}'\"",
      "\t [dataReader]=\"'${3:dataReader}'\"",
      "\t [displayField]=\"'${4:displayField}'\"",
      "\t [valueField]=\"'${5:valueField}'\"",
      "\t [radioGroupBindData]=\"radioGroupData\"",
      "\t (selectedValue)=\"setSelectedGender($${7:event})\"",
      "\t [column]=\"'${8:column}'\">",
      " </amexio-radio-group>"
    ],
    "description": "Amexio Vertical Radio Group"
  },
  //type ahead
  "amexio-text-type-ahead": {
    "prefix": "amexio-text-type-ahead",
    "body": [
      "<amexio-typeahead [dataReader]=\"'response.data'\"",
      "\t [httpUrl]=\"'${1:httpUrl}'\"",
      "\t [httpMethod]=\"'${2:httpMethod}'\"",
      "\t [key]=\"'${3:key}'\"",
      "\t [(ngModel)]=\"${4:ngModel}\"",
      "\t [fieldLabel]=\"'${5:fieldLabel}'\">",
      "</amexio-typeahead>"
    ],
    "description": "Amexio Text Type Ahead"
  },
  // Button Snippets
  "amexio-type-button": {
    "prefix": "amexio-type-button",
    "body": [
      "<amexio-btn [label]=\"'${1:label}'\"",
      "\t [type]=\"'${2:type}'\"",
      "\t [tooltipMessage]=\"''${3:tooltipMessage}'\">",
      "</amexio-btn>"
    ],
    "description": "Amexio Type Button"
  },
  "amexio-state-button": {
    "prefix": "amexio-state-button",
    "body": [
      "<amexio-btn [label]=\"'${1:label}'\"",
      "\t [type]=\"'primary'\"",
      "\t [tooltipMessage]=\"''${2:tooltipMessage}''\"",
      "\t  [disabled]=\"'${3:disabled}'\">",
      "\t </amexio-btn>"
    ],
    "description": "Amexio State Button"
  },
  "amexio-async-button": {
    "prefix": "amexio-async-button",
    "body": [
      "<amexio-btn [label]=\"'${1:label}'\"",
      "\t [type]=\"'primary'\"",
      "\t [tooltipMessage]=\"'${3:tooltipMessage}'\"",
      "\t (onClick)=\"'${4:onClickMethod}'\"",
      "\t [isLoading]=\"'${5:asyncFlag}'\">",
      "\t </amexio-btn>"
    ],
    "description": "Amexio Async Button"
  },
  "amexio-icon-button": {
    "prefix": "amexio-icon-button",
