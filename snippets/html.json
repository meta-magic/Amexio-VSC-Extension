		"prefix": "amexio-spinner",
		"body": [
			"<amexio-spinner", 
			"\t[show]=\"clickShow\"", 
			"\t[type]=\"'${1:type}'\"", 
			"\t[color]=\"'red'\"", 
			"\t[size]=\"'16px'\">",
			"\t</amexio-spinner>"

		],
		"description": "Amexio Spinner"
	},
	//Data
	//Data Item Selector
	"amexio-data-item-selector": {
		"prefix": "amexio-data-item-selector",
		"body": [
			"<amexio-item-selector",
			"\t[height]=\"'${1:height}'\"",
			"\t[display-field]=\"'${2:feild1}'\"",
			"\t[value-field]=\"'${3:feild2}'\"",
			"\t(selectedRecords)=\"${4:getSelectedData}(\\$event)\"",
			"\t[http-url]=\"'${5:path}'\"",
			"\t[http-method]=\"'get'\"",
			"\t[data-reader]=\"'${6:data}'\">",
			"</amexio-item-selector>"
		],
		"description": "Amexio Item Data Selector"
	},
	"amexio-timeline": {
		"prefix": "amexio-timeline",
		"body": [
			"<amexio-timeline",
			"\t[content-alignment]=\"'${1:content-alignment}'\">",
			"<amexio-timeline-event",
			"\t[content-alignment]=\"'${1:content-alignment}'\">",
			"\t[content-border]=\"false\"",
			"\t[label]=\"'${3:label}'\"",
			"\t[icon]=\"'${4:icon}'\">",
			"</amexio-timeline-event>",
			"</amexio-timeline>"
		],
		"description": "Amexio Item Data Selector"
	},
	"amexio-timeline-event": {
		"prefix": "amexio-timeline-event",
		"body": [
			"<amexio-timeline-event",
			"\t[content-alignment]=\"'${1:content-alignment}'\">",
			"\t[content-border]=\"false\"",
			"\t[label]=\"'${3:label}'\"",
			"\t[icon]=\"'${4:icon}'\">",
			"</amexio-timeline-event>"
		],
		"description": "Amexio Item Data Selector"
	},
	//Data-Grid
	"amexio-data-grid-simple": {
		"prefix": "amexio-data-grid-simple",
		"body": [
			"<amexio-datagrid",
			"\t[enable-column-fiter]=\"true\"",
			"\ttitle=\"'${1:title}'\"",
			"\t[http-method]=\"'get'\"",
			"\t[http-url]=\"'${2:path}'\"",
			"\t[data-reader]=\"'${3:data}'\"",
			"\t[page-size] = \"${4:page-size}\"",
			"\t(selectedRowData)=\"${5:onSelectRow}(\\$event)\">",
			"\t<amexio-data-table-column",
			"\t\t[data-index]=\"'${6:columnName}'\"",
			"\t\t[data-type]=\"'string'\"",
			"\t\t[hidden]=\"false\"",
			"\t\t[text]=\"'${7:name}'\">",
			"\t</amexio-data-table-column>",
			"</amexio-datagrid>"
		],
		"description": "Amexio Simple Data Grid "
	},
	//Data-Grid-Column
	"amexio-data-grid-column": {
		"prefix": "amexio-data-grid-column",
		"body": [
			"<amexio-data-table-column",
			"\t[data-index]=\"'${1:columnName}'\"",
			"\t[data-type]=\"'string'\"",
			"\t[hidden]=\"false\"",
			"\t\t[text]=\"'${2:name}'\">",
			"</amexio-data-table-column>"
		],
		"description": "Amexio Data Grid Column "
	},
	//Data-Grid-Template
	"amexio-data-grid-template": {
		"prefix": "amexio-data-grid-template",
		"body": [
			"<amexio-datagrid",
			"\ttitle=\"'${1:title}'\"",
			"\t[http-method]=\"'get'\"",
			"\t[http-url]=\"'${2:path}'\"",
			"\t[data-reader]=\"'${3:data}'\"",
			"\t[page-size] = \"${4:pagesize}\"",
			"\t[enable-data-filter]=\"false\"",
			"\t(rowSelect)=\"'${5:getRowData}(\\$event)'\">",
			"\t<amexio-data-table-column",
			"\t\t[width]=\"${6:width}\"",
			"\t\t[data-index]=\"'${7:dataIndex}'\"",
			"\t\t[data-type]=\"'string'\"",
			"\t\t[hidden]=\"false\"",
			"\t\t[text]=\"'${8:name}'\">",
			"\t\t<ng-template #amexioBodyTmpl \"${9:loop}\">",
			"\t\t ${10:Content} ",
			"\t\t</ng-template>",
			"\t</amexio-data-table-column>",
			"</amexio-datagrid>"
		],
		"description": "Amexio Data Grid Template"
	},
	//Data-Grid-Header-Template
	"amexio-data-grid-header-template": {
		"prefix": "amexio-data-grid-header-template",
		"body": [
			"<amexio-datagrid",
			"\ttitle=\"'${1:title}'\"",
			"\t[http-method]=\"'get'\"",
			"\t[http-url]=\"'${2:path}'\"",
			"\t[data-reader]=\"'${3:data}'\"",
			"\t[page-size] = \"${4:pagesize}\"",
			"\t[enable-data-filter]=\"false\"",
			"\t(rowSelect)=\"'${5:getRowData}(\\$event)'\">",
			"\t<amexio-data-table-column",
			"\t[width]=\"${6:width}\"",
			"\t\t[data-index]=\"'${7:dataIndex}'\"",
			"\t\t[data-type]=\"'string'\"",
			"\t\t[hidden]=\"false\"",
			"\t\t[text]=\"'${8:name}'\">",
			"\t\t<ng-template #amexioHeaderTmpl let-column=\"column\" let-index=\"index\">",
			"\t\t\t\t${9:content}",
			"\t\t</ng-template>",
			"\t</amexio-data-table-column>",
			"</amexio-datagrid>"
		],
		"description": "Amexio Data Grid Header Template "
	},
	// Data-Grid-Filter
	"amexio-data-grid-filter": {
		"prefix": "amexio-data-grid-filter",
		"body": [
			"<amexio-datagrid",
			"\t[title]=\"'${1:title}'\"",
			"\t[page-size] = \"'${2:page-size}'\"",
			"\t[data-reader]=\"'${3:data}'\"",
			"\t[http-url]=\"'${4:path}'\"",
			"\t[http-method]=\"'get'\"",
			"\t[enable-data-filter]=\"true\"",
			"\t(selectedRowData)=\"${5:onRowSelect}(\\$event)\">",
			"\t\t<amexio-data-table-column",
			"\t\t\t[data-index]=\"'${6:columnName}'\"",
			"\t\t\t[data-type]=\"'string'\"",
			"\t\t\t[hidden]=\"false\"",
			"\t\t\t[text]=\"'${7:name}'\">",
			"\t\t</amexio-data-table-column>",
			"</amexio-datagrid>"
		],
		"description": "Amexio Filter Grid Data "
	},
	// Data-Grid-Global-Filter
	"amexio-data-grid-filter": {
		"prefix": "amexio-data-grid-filter",
		"body": [
			"<amexio-datagrid",
			"\t[global-filter]=\"true\"",
			"\t[title]=\"'${1:title}'\"",
			"\t[page-size] = \"'${2:page-size}'\"",
			"\t[data-reader]=\"'${3:data}'\"",
			"\t[http-url]=\"'${4:path}'\"",
			"\t[http-method]=\"'get'\"",
			"\t[enable-data-filter]=\"true\"",
			"\t(selectedRowData)=\"${5:onRowSelect}(\\$event)\">",
			"\t\t<amexio-data-table-column",
			"\t\t\t[data-index]=\"'${6:columnName}'\"",
			"\t\t\t[data-type]=\"'string'\"",
			"\t\t\t[hidden]=\"false\"",
			"\t\t\t[text]=\"'${7:name}'\">",
			"\t\t</amexio-data-table-column>",
			"</amexio-datagrid>"
		],
		"description": "Amexio Data Grid Global Filter "
	},
	//Data-Groupby-Grid
	"amexio-data-groupby-grid": {
		"prefix": "amexio-data-groupby-grid",
		"body": [
			"<amexio-datagrid",
			"\t[title]=\"'${1:title}'\"",
			"\t[page-size] =\"${2:page-size}\"",
			"\t[data-reader]=\"'${3:data}'\"",
			"\t[groupby]=\"true\"",
			"\t[groupby-data-index]=\"'${4:groubyColumnName}'\"",
			"\t[http-url]=\"'${5:path}'\"",
			"\t[http-method]=\"'get'\"",
			"\t(selectedRowData)=\"${6:onRowSelect}($event)\">",
			"\t<amexio-data-table-column [data-index]=\"'${7:columnName}'\"",
			"\t\t[data-type]=\"'string'\"",
			"\t\t[hidden]=\"false\"",
			"\t\t[text]=\"'${8:name}'\">",
			"\t</amexio-data-table-column>",
			"</amexio-datagrid>"
		],
		"description": "Amexio data grid Groupby"
	},
	//Data-Scroll-Groupby
	"amexio-Scroll-data-grid": {
		"prefix": "amexio-scroll-data-grid",
		"body": [
			"<amexio-datagrid",
			"\t[enable-column-fiter]=\"true\" [height]=\"${1:height}\" title=\"${2:title}\"",
			"\t[enable-checkbox]=\"false\"",
			"\t[http-method]=\"'get'\"",
			"\t[http-url]=\"'${3:path}'\"",
			"\t[data-reader]=\"'${4:data}'\" ",
			"\t[page-size] = \"${5:page-size}\">",
			"\t<amexio-data-table-column",
			"\t\t[data-index]=\"'${6:dataindex}'\"",
			"\t\t[data-type]=\"'string'\"",
			"\t\t[hidden]=\"false\"",
			"\t\t[text]=\"'name'\">",
			"\t</amexio-data-table-column>",
			"</amexio-datagrid>"
		],
		"description": "Amexio Data Grid With Scroll"
	},
	//Data-Grid-with-context-menu
	"amexio-data-grid-with-context-menu": {
		"prefix": "amexio-data-grid-with-context-menu",
		"body": [
			"<amexio-datagrid",
			"\t[context-menu]=\"${1:data}\"",
			"\t[enable-column-fiter]=\"true\"",
			"\t[height]=\"${2:height}\"",
			"\ttitle=\"${3:title}\"",
			"\t[enable-checkbox]=\"false\"",
			"\t[http-method]=\"'get'\"",
			"\t[http-url]=\"'${4:path}'\"",
			"\t[data-reader]=\"'${5:data}'\" ",
			"\t[page-size] = \"${6:page-size}\">",
			"\t<amexio-data-table-column",
			"\t\t[data-index]=\"'${7:dataindex}'\"",
			"\t\t[context-menu]=\"${8:data}\"",
			"\t\t[data-type]=\"'string'\"",
			"\t\t[hidden]=\"false\"",
			"\t\t[text]=\"'name'\">",
			"\t</amexio-data-table-column>",
			"</amexio-datagrid>"
		],
		"description": "Amexio Data Grid With Context Menu"
	},
	//Grid-Treedata
	"amexio-treedata-grid": {
		"prefix": "amexio-treedata-grid",
		"body": [
			"<amexio-tree-data-table",
			"\t[http-method]=\"'get'\"",
			"\t[http-url]=\"'${1:path}'\"",
			"\t[data-reader]=\"'${2:data}'\"",
			"\t(selectedRecord)=\"${3:getSelectedData}(\\$event)\">",
			"\t<amexio-data-table-column",
			"\t\t[data-index]=\"'${4:dataindex}'\"",
			"\t\t[data-type]=\"'string'\"",
			"\t\t[hidden]=\"false\"",
			"\t\t[text]=\"'{5:name}'\">",
			"\t</amexio-data-table-column>",
			"\t<amexio-data-table-column ",
			"\t\t[data-index]=\"'${6:dataindex}'\"",
			"\t\t[data-type]=\"'number'\"",
			"\t\t[hidden]=\"false\"",
			"\t\t[text]=\"'${7:Name}'\">",
			"\t</amexio-data-table-column>",
			"</amexio-tree-data-table>"
		],
		"description": "Amexio Data TreeGrid"
	},
	//Grid-Tree-data-template
	"amexio-treedata-template": {
		"prefix": "amexio-treedata-template",
		"body": [
			"<amexio-tree-data-table",
			"\t[http-method]=\"'get'\"",
			"\t[http-url]=\"'${1:path}'\"",
			"\t[data-reader]=\"'${2:data}'\"",
			"\t(selectedRecord)=\"${3:getSelectedData}(\\$event)\">",
			"\t<amexio-data-table-column",
			"\t\t[data-index]=\"'${4:dataindex}'\"",
			"\t\t[data-type]=\"'string'\"",
			"\t\t[hidden]=\"false\"",
			"\t\t[text]=\"'{5:name}'\">",
			"\t</amexio-data-table-column>",
			"\t<amexio-data-table-column ",
			"\t\t[data-index]=\"'${6:dataindex}'\"",
			"\t\t[data-type]=\"'number'\"",
			"\t\t[hidden]=\"false\"",
			"\t\t[text]=\"'${7:name}'\">",
			"\t\t\t <ng-template #amexioBodyTmpl let-column let-row=\"row\" >",
			"\t\t\t\t\t ${8:context}",
			"\t\t\t </ng-template>",
			"\t</amexio-data-table-column>",
			"</amexio-tree-data-table>"
		],
		"description": "Amexio Tree Data Template"
	},
	//Tree
	//Tree Simple    
	"amexio-treeview": {
		"prefix": "amexio-treeview",
		"body": [
			"<amexio-treeview [data-reader]=\"'${1:data}'\" [data]=\"${2:treeLocalData}\" (nodeClick)=\"${3:getNodeData}(\\$event)\">",
			"</amexio-treeview>"
		],
		"description": "Amexio  Data Treeview"
	},
	//Treeview templete
	"amexio-treeview-template": {
		"prefix": "amexio-treeview-template",
		"body": [
			"\t<amexio-treeview [data-reader]=\"'${1:data}'\"",
			"\t\t\t\t[http-url]=\"'${2:path}'\"",
			"\t\t\t\t[http-method]=\"'get'\"",
			"\t\t\t\t(nodeClick)=\"${3:getNodeData}(\\$event)\">",
			"\t\t<ng-template #amexioTreeTemplate \"${4:loop}\">",
			"\t\t\t ${5:Content}",
			"\t</ng-template>",
			"</amexio-treeview>"
		],
		"description": "Amexio tree view with template"
	},
	//Tree Filter View
	"amexio-tree-filter-view": {
		"prefix": "amexio-tree-filter-view",
		"body": [
			"<amexio-tree-filter-view",
			"\t [data-reader]=\"'${1:data}'\"",
			"\t[http-method]=\"'get'\"",
			"\t(selectedRecord)=\"${2:getNodeData}(\\$event)\"",
			"\t[http-url]=\"'${3:path}'\">",
			"</amexio-tree-filter-view>"
		],
		"description": "Amexio filter Tree View Data"
	},
	//Tree Checkbox
	"amexio-tree-checkbox": {
		"prefix": "amexio-tree-checkbox",
		"body": [
			"<amexio-treeview",
			"\t[enable-checkbox]=\"true\"",
			"\t[data-reader]=\"'${1:data}'\"",
			"\t[data]=\"${2:treeLocalData}\"",
			"\t(onTreeNodeChecked)=\"${3:getNodeData}(\\$event)\">",
			"</amexio-treeview>"
		],
		"description": "Amexio tree checbox"
	},
	//Tree drag drop
	"amexio-tree-drag-drop": {
		"prefix": "amexio-tree-drag-drop",
		"body": [
			"<amexio-treeview",
			"\t[enable-drag]=\"true\"",
			"\t[enable-drop]=\"true\"",
			"\t[across-tree]=\"true\"",
			"\t[data-reader]=\"'${1:data}'\"",
			"\t[data]=\"${2:treeLocalData}\"",
			"\t(onTreeNodeChecked)=\"${3:getNodeData}(\\$event)\">",
			"</amexio-treeview>"
		],
		"description": "Amexio tree drag drop"
	},
	//Tree with badge
	"amexio-tree-with-badge": {
		"prefix": "amexio-tree-with-badge",
		"body": [
			"<amexio-treeview",
			"\t[badge]=\"true\"",
			"\t[data-reader]=\"'${1:data}'\"",
			"\t[data]=\"${2:treeLocalData}\"",
			"\t(onTreeNodeChecked)=\"${3:getNodeData}(\\$event)\">",
			"</amexio-treeview>"
		],
		"description": "Amexio tree with badge"
	},
	//Tree with context menu
	"amexio-tree-with-context-menu": {
		"prefix": "amexio-tree-with-context-menu",
		"body": [
			"<amexio-treeview [context-menu]=\"${1:data}\"",
			"\t[data-reader]=\"'${2:data}'\"",
			"\t[data]=\"${3:treeLocalData}\"",
			"\t(onTreeNodeChecked)=\"${4:getNodeData}(\\$event)\">",
			"</amexio-treeview>"
		],
		"description": "Amexio tree with context-menu"
	},
	// Horizontal Tree View
	"amexio-treeview-horizontal": {
		"prefix": "amexio-tree-horizontal",
		"body": [
			"<amexio-horizontal-treeview",
			"\t[data-reader]=\"'${1:data}'\"",
			"\t[http-method]=\"'get'\"",
			"\t[label]=\"'${2:label}'\"",
			"\t(nodeClick)=\"${3:getNodeData}(\\$event)\"",
			"\t[http-url]=\"'${4:path}'\">",
			"</amexio-horizontal-treeview>"
		],
		"description": "Amexio TreeView Horizontal"
	},
	//Drag and Drop
	"amexio-within-tree-drag-drop": {
		"prefix": "amexio-within-tree-drag-drop",
		"body": [
			"<amexio-treeview",
			"\t[data-reader]=\"'${1:data}'\"",
			"\t[enable-drag]=\"true\"",
			"\t[enable-drop]=\"true\"",
			"\t(nodeClick)=\"${2:getNodeData}(\\$event)\"",
			"\t[data]=\"'${3:data}'\">",
			"</amexio-treeview>"
		],
		"description": "Amexio Within Tree Drag And Drop"
	},
	"amexio-across-tree-drag-drop": {
		"prefix": "amexio-across-tree-drag-drop",
		"body": [
			"<amexio-treeview #tree",
			"\t[data-reader]=\"'${1:data}'\"",
			"\t[enable-drag]=\"true\"",
			"\t[enable-drop]=\"true\"",
			"\t [across-tree]=\"true\"",
			"\t(nodeClick)=\"${2:getNodeData}(\\$event)\"",
			"\t[data]=\"'${3:data}'\">",
			"</amexio-treeview>"
		],
		"description": "Amexio Across Tree Drag And Drop"
	},
	"amexio-tree-to-panel": {
		"prefix": "amexio-tree-to-panel",
		"body": [
			"<amexio-treeview",
			"\t[data-reader]=\"'${1:data}'\"",
			"\t[enable-drag]=\"true\"",
			"\t[enable-drop]=\"true\"",
			"\t[across-tree]=\"true\"",
			"\t(onDrag)=\"${2:drag}(\\$event)\"",
			"\t[data]=\"'${3:data}'\">",
			"</amexio-treeview>",
			"<amexio-panel",
			"\t(dragover)=\"${4:dragOver}(\\$event)\"",
			"\t(drop)=\"${5:drop}(\\$event)\"",
			"\t[header]=\"${6:true}\"",
			"\t[title]=\"'${7:title}'\"",
			"\t[expanded]=\"true\">",
			"\t\t\t${8:content}",
			"</amexio-panel>"
		],
		"description": "Amexio Across Tree Drag And Drop"
	},
	//Data Listbox
	"amexio-data-listbox": {
		"prefix": "amexio-data-listbox",
		"body": [
			"<amexio-listbox",
			"\t[enable-checkbox]=\"true\"",
			"\t[height]=\"${1:height}\"",
			"\t[header]=\"'${2:header}'\"",
			"\t[enable-header]=\"${3:true}\"",
			"\t[search-placeholder]=\"'${4:placeholder}'\"",
			"\t[data]=\"'${5:localData}'\"",
			"\t[filter]=\"true\"",
			"\t[data-reader]=\"'${6:data-reader}'\"",
			"\t[display-field]=\"'${7:diaplayField}'\"",
			"\t[http-url]=\"'${8:url}'\"",
			"\t[http-method]=\"'${9:http-method}'\"",
			"\t[search-placeholder]=\"'${10:search-placeholder}'\"",
			"\t[context-menu]=\"${11:context-menu}\"",
			"\t(selectedRows)=\"'${12:getSelectedData}(\\$event)\">",
			"</amexio-listbox>"
		],
		"description": "amexio data listbox"
	},
	//Data Listbox Template
	"amexio-listbox-template": {
		"prefix": "amexio-listbox-template",
		"body": [
			"<amexio-listbox",
			"\t[height]=\"'${1:height}'\"",
			"\t[header]=\"'${2:header}'\"",
			"\t[search-placeholder]=\"'${3:Search Placeholder}'\"",
			"\t[data]=\"${3:localData}\"",
			"\t[filter]=\"'false'\"",
			"\t[enable-checkbox]=\"'false'\"",
			"\t[data-reader]=\"'${4:data-reader}'\"",
			"\t[display-field]=\"'${5:name}'\"",
			"\t(onRowClick)=\"'${6:onRowClick}(\\$event)'\">",
			"\t<ng-template #amexioBodyTmpl let-row=\"row\">",
			"\t${7:Content}",
			"\t</ng-template>",
			"</amexio-listbox>"
		],
		"description": "Amexio listbox template"
	},
	// D3 Chart Snippets
	"amexio-d3-chart-bar": {
		"prefix": "amexio-d3-chart-bar",
		"body": [
			"<amexio-d3-chart-bar", 
        "\t[width]=\"${1:width}\"",
        "\t[data]=\"${2:data}\"", 
        "\t[height]=\"{3:height}\"", 
        "\t[title]=\"'{4:title}'\"",
        "\t[label-color]=\"'white'\"",
        "\t[label]=\"false\"",
        "\t(onChartClick)=\"onDefaultChartClick(\\$event)\"",
        "\t(onLegendClick)=\"onUserDefineLegendClick(\\$event)\"",
        "\t[color]=\"userDefineColors\">",
 		"</amexio-d3-chart-bar>"
		],
		"description": "Amexio D3 Bar Chart"
	},
	"amexio-d3-chart-bar-horizontal": {
		"prefix": "amexio-d3-chart-bar-horizontal",
		"body": [
			"<amexio-d3-chart-bar", 
			"\t[horizontal]=\"true\"", 
			"\t[label]=\"true\"",
