	"amexio-card-image": {
		"prefix": "amexio-card-image",
		"body": [
			"<amexio-card",
			"\t[header]=\"${1:false}\"",
			"\t[header-align]=\"'${2:left}'\"",
			"\t[footer]=\"${3:false}\"",
			"\t[footer-align]=\"'${4:left}'\"",
			"\t[show]=\"${5:true}\"",
			"\t[body-height]=\"'${6:height}'\"",
			"\t[enable-lifecycle-events]=\"${7:lifecycle-events}\"",
			"\t(rightClick)=\"${8:functionName}(\\$event)\"",
			"\t(initiated)=\"${9:functionName}(\\$event)\"",
			"\t(ready)=\"${10:functionName}(\\$event)\"",
			"\t(destroy)=\"${11:functionName}(\\$event)\">",
			"\t\t<amexio-header>",
			"\t\t${12:header}",
			"\t\t</amexio-header>",
			"\t\t<amexio-body>",
			"\t\t\t<amexio-image path=\"${13:image}\">",
			"\t\t\t</amexio-image> ",
			"\t\t</amexio-body>",
			"\t\t<amexio-action>",
			"\t\t\t${14:footer}",
			"\t\t</amexio-action>",
			"</amexio-card>"
		],
