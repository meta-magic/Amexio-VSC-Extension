{
  //Text Input 
  "amexio-text": {
    "prefix": "amexio-text",
    "body": [
      "<amexio-text-input [fieldLabel]=\"'${1:label}'\" name =\"${2:name}\"",
      "\t [placeholder]=\"'${3:placeHolder}'\"",
      "\t [iconFeedBack]=\"true\">",
      "</amexio-text-input>"
    ],
    "description": "Amexio Simple Text Input"
  },
  "amexio-text-input-no-label": {
    "prefix": "amexio-text-input-no-label",
    "body": [
      "<amexio-text-input name =\"'${1:name}'\"",
      "\t [placeholder]=\"'${2:placeholder}'\"",
      "\t [iconFeedBack]=\"false\"",
      "\t [hasLabel]=\"false\">",
      "\t </amexio-text-input>"
    ],
    "description": "Amexio Text Input without Label"
  },
  "amexio-text-input-validations": {
    "prefix": "amexio-text-input-validations",
    "body": [
      "<amexio-text-input [fieldLabel]=\"'${1:label}'\" name =\"${2:name}\"",
      "\t [placeholder]=\"'${3:placeHolder}'\"",
      "\t [allowBlank]=\"false\"",
      "\t [errorMsg] =\"'${4:errorMsg}'\"",
      "\t [minLength]=\"${5:minLength}\"",
      "\t [minErrorMsg]=\"'${6:minErrorMsg}'\"",
      "\t [maxLength]=\"${7:maxLength}\"",
      "\t [maxErrorMsg]=\"'${8:minErrorMsg}'\"",
      "\t [iconFeedBack]=\"true\">",
      "\t </amexio-text-input>"
    ],
    "description": "Amexio Text Input with Validations"
  },
  "amexio-text-input-disabled": {
    "prefix": "amexio-text-input-disabled",
