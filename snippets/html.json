	//Layout
	//Layout-accordian
	"amexio-accordion": {
		"prefix": "amexio-accordion",
		"body": [
			"<amexio-accordion",
			"\t[angle-icon]=\"${1:false}\"",
			"\t\t(initiated)=\"${2:functionName}(\\$event)\"",
			"\t\t(ready)=\"${3:functionName}(\\$event)\"",
			"\t\t(destroy)=\"${4:functionName}(\\$event)\">",
			"\t<amexio-accordion-tab",
			"\t\theader=\"${5:header}\"",
			"\t\t[active]=\"${6:true}\" ",
			"\t\tleft-icon=\"${7:icon}\"",
			"\t\t[angle-icon]=\"${8:false}\"",
			"\t\t[disabled]=\"${9:false}\"",
			"\t\t(emittedEvent)=\"${10:functionName}(\\$event)\">",
			"\t\t\t${11:Content}",
			"\t</amexio-accordion-tab>",
			"</amexio-accordion>"
		],
		"description": "Amexio Accordian Pane"
	},
	"amexio-accordion-tab": {
		"prefix": "amexio-accordion-tab",
		"body": [
			"\t<amexio-accordion-tab ",
			"\t\theader=\"${1:header}\"",
			"\t\t[active]=\"${2:true}\" ",
			"\t\tleft-icon=\"${3:icon}\"",
			"\t\tangle-icon=\"${4:false}\"",
			"\t\tdisabled=\"${5:false}\"",
			"\t\t(emittedEvent)=\"${6:functionName}(\\$event)\">",
			"\t\t\t${7:Content}",
			"\t</amexio-accordion-tab>"
		],
		"description": "Amexio Accordian Pane Tab"
	},
	"amexio-accordion-expand-all": {
		"prefix": "amexio-accordion-expand-all",
		"body": [
			"<amexio-accordion  expand-all=\"true\" [angle-icon]=\"${1:true}\">",
			"\t<amexio-accordion-tab ",
			"\t\theader=\"${2:header}\"",
			"\t\t[active]=\"${3:true}\" ",
			"\t\tleft-icon=\"${4:icon}\"",
			"\t\t[angle-icon]=\"${5:false}\"",
			"\t\tdisabled=\"${6:false}\"",
			"\t\t(emittedEvent)=\"${7:functionName}(\\$event)\">",
			"\t\t\t${8:Content}",
			"\t</amexio-accordion-tab>",
			"</amexio-accordion>"
		],
		"description": "Amexio Accordian Pane Expand All"
	},
	"amexio-accordion-transparent": {
		"prefix": "amexio-accordion-transparent",
		"body": [
			"<amexio-accordion transparent=\"true\" [angle-icon]=\"${1:true}\"",
			"background=\"${2:background}\" color=\"${3:color}\" enable-lifecycle-events=\"${4:enable-lifecycle-events}\">",
			"\t<amexio-accordion-tab ",
			"\t\theader=\"${2:header}\"",
			"\t\t[active]=\"${3:true}\" ",
			"\t\tleft-icon=\"${4:icon}\"",
			"\t\t[angle-icon]=\"${5:false}\"",
			"\t\tdisabled=\"${6:false}\"",
			"\t\t(emittedEvent)=\"${7:functionName}(\\$event)\">",
			"\t\t\t${8:Content}",
			"\t</amexio-accordion-tab>",
			"</amexio-accordion>"
		],
		"description": "Amexio Accordian Pane Transperent"
	},
	//Layout-border
	"amexio-border-layout": {
		"prefix": "amexio-border-layout",
		"body": [
			"<amexio-borderlayout>",
			"\t<amexio-borderlayout-item position=\"${1:position}\">",
			"\t\t${2:Content}",
			"\t</amexio-borderlayout-item>",
			"</amexio-borderlayout>"
		],
		"description": "Amexio Border Layout "
	},
	"amexio-border-layout-item": {
		"prefix": "amexio-border-layout-item",
		"body": [
			"\t<amexio-borderlayout-item position=\"${1:position}\">",
			"\t\t${2:Content}",
			"\t</amexio-borderlayout-item>"
		],
		"description": "Amexio Border Layout Item"
	},
	//Layout-column
	"amexio-row": {
		"prefix": "amexio-row",
		"body": [
			"<amexio-row>",
			"\t",
			"</amexio-row>"
		],
		"description": "Amexio Row"
	},
	"amexio-column": {
		"prefix": "amexio-column",
		"body": [
			"<amexio-column", 
			"\t\t\t\tsize=\"${1:size}\"",
			"\t\t\t\tfit=\"${2:false}\">",
			"</amexio-column>"
		],
		"description": "Amexio Column [size] :: 2:4:6:12"
	},
	//box
	"amexio-box": {
		"prefix": "amexio-box",
		"body": [
			"<amexio-box",
			"\tborder-color =\"${1:border-color}\"",
			"\tborder=\"${2:borderPosition}\"",
			"\t[border-dotted]=\"${3:false}\"",
			"\t[padding]=\"false\"",
			"\t[closable]=\"${4:false}\"",
			"\talign=\"${5:left}\">",
			"\t\t${6:Content}",
			"</amexio-box>"
		],
		"description": "Amexio Box "
	},
	"amexio-box-background": {
		"prefix": "amexio-box-background",
		"body": [
			"<amexio-box",
			"\tborder-color =\"${1:border-color}\"",
			"\tborder=\"${2:borderPosition}\"",
			"\t[border-dotted]=\"${3:false}\"",
			"\tbackground-color=\"${4:background-color}\"",
			"\talign=\"${5:left}\"",
			"\t[closable]=\"${6:false}\"",
			"\t[padding]=\"false\">",
			"\t\t${7:Content}",
			"</amexio-box>"
		],
		"description": "Amexio Box Background"
	},
	"amexio-box-background-height-width": {
		"prefix": "amexio-box-background-height-width",
		"body": [
			"<amexio-box",
			"\tborder-color =\"${1:border-color}\"",
			"\tborder=\"${2:borderPosition}\"",
			"\t[border-dotted]=\"${3:false}\"",
			"\tbackground-color=\"${4:background-color}\"",
			"\tbox-height=\"${5:height}\"",
			"\tbox-width=\"${6:width}\"",
			"\talign=\"${7:left}\"",
			"\t[closable]=\"${8:false}\"",
			"\t[padding]=\"true\">",
			"\t\t${9:Content}",
			"</amexio-box>"
		],
		"description": "Amexio Box with background height and width "
	},
	//Layout-card
	"amexio-card": {
		"prefix": "amexio-card",
		"body": [
			"<amexio-card",
			"\t[header]=\"${1:false}\"",
			"\t[header-align]=\"'${2:left}'\"",
			"\t[footer]=\"${3:false}\"",
			"\t[footer-align]=\"'${4:left}'\"",
			"\t[show]=\"'${5:showVariable}'\"",
			"\theight=\"${6:height}\"",
			"\t(rightClick)=\"${7:functionName}(\\$event)\"",
			"\t(initiated)=\"${8:functionName}(\\$event)\"",
			"\t(ready)=\"${9:functionName}(\\$event)\"",
			"\t(destroy)=\"${10:functionName}(\\$event)\">",
			"\t\t<amexio-header>",
			"\t\t\t${11:header}",
			"\t\t</amexio-header>",
			"\t\t<amexio-body>",
			"\t\t\t${12:body}",
			"\t\t</amexio-body>",
			"\t\t<amexio-action>",
			"\t\t\t${13:footer}",
			"\t\t</amexio-action>",
			"</amexio-card>"

		],
		"description": "Amexio Card "
	},
	"amexio-card-with-height": {
		"prefix": "amexio-card-with-height",
		"body": [
			"<amexio-card",
			"\t[header]=\"${1:true}\"",
			"\t[header-align]=\"'${2:right}'\"",
			"\t[footer]=\"${3:false}\"",
			"\t[footer-align]=\"'${4:right}'\"",
			"\t[show]=\"'${5:showVariable}'\"",
			"\t[body-height]=\"'${6:height}'\"",
			"\t[min-height]=\"${7:height}\"",
			"\t[height]=\"${8:height}\"",
			"\t(rightClick)=\"${9:functionName}(\\$event)\"",
			"\t(initiated)=\"${10:functionName}(\\$event)\"",
			"\t(ready)=\"${11:functionName}(\\$event)\"",
			"\t(destroy)=\"${12:functionName}(\\$event)\">",
			"\t\t<amexio-header>",
			"\t\t\t${12:header}",
			"\t\t</amexio-header>",
			"\t\t<amexio-body>",
			"\t\t\t${13:body}",
			"\t\t</amexio-body>",
			"\t\t<amexio-action>",
			"\t\t\t${14:footer}",
			"\t\t</amexio-action>",
			"</amexio-card>"
		],
		"description": "Amexio Card with Height"
	},
	"amexio-card-with-context-menu": {
		"prefix": "amexio-card-with-context-menu",
