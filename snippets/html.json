	//Layout
	//Layout-accordian
	"amexio-accordion": {
		"prefix": "amexio-accordion",
		"body": [
			"<amexio-accordion",
			"\t[angle-icon]=\"${1:false}\">",
			"\t<amexio-accordion-tab ",
			"\t\theader=\"${2:header}\"",
			"\t\t[active]=\"${3:true}\" ",
			"\t\tleft-icon=\"${4:icon}\"",
			"\t\t[angle-icon]=\"${5:false}\"",
			"\t\t[disabled]=\"${6:false}\"",
			"\t\t(emittedEvent)=\"${7:functionName}(\\$event)\">",
			"\t\t\t${8:Content}",
			"\t</amexio-accordion-tab>",
			"</amexio-accordion>"
		],
		"description": "Amexio Accordian Pane"
	},
	"amexio-accordion-tab": {
		"prefix": "amexio-accordion-tab",
		"body": [
			"\t<amexio-accordion-tab ",
			"\t\theader=\"${1:header}\"",
			"\t\t[active]=\"${2:true}\" ",
			"\t\tleft-icon=\"${3:icon}\"",
			"\t\t[angle-icon]=\"${4:false}\"",
			"\t\t[disabled]=\"${5:false}\"",
			"\t\t(emittedEvent)=\"${6:functionName}(\\$event)\">",
			"\t\t\t${7:Content}",
			"\t</amexio-accordion-tab>"
		],
		"description": "Amexio Accordian Pane Tab"
	},
	"amexio-accordion-expand-all": {
		"prefix": "amexio-accordion-expand-all",
		"body": [
			"<amexio-accordion  expand-all=\"true\" angle-icon=\"${1:true}\">",
			"\t<amexio-accordion-tab ",
			"\t\theader=\"${2:header}\"",
			"\t\t[active]=\"${3:true}\" ",
			"\t\tleft-icon=\"${4:icon}\"",
			"\t\t[angle-icon]=\"${5:false}\"",
			"\t\t[disabled]=\"${6:false}\"",
			"\t\t(emittedEvent)=\"${7:functionName}(\\$event)\">",
			"\t\t\t${8:Content}",
			"\t</amexio-accordion-tab>",
			"</amexio-accordion>"
		],
		"description": "Amexio Accordian Pane Expand All"
	},
	"amexio-accordion-transparent": {
		"prefix": "amexio-accordion-transparent",
		"body": [
			"<amexio-accordion transparent=\"true\" angle-icon=\"${1:true}\">",
			"\t<amexio-accordion-tab ",
			"\t\theader=\"${2:header}\"",
			"\t\t[active]=\"${3:true}\" ",
			"\t\tleft-icon=\"${4:icon}\"",
			"\t\t[angle-icon]=\"${5:false}\"",
			"\t\t[disabled]=\"${6:false}\"",
			"\t\t(emittedEvent)=\"${7:functionName}(\\$event)\">",
			"\t\t\t${8:Content}",
			"\t</amexio-accordion-tab>",
			"</amexio-accordion>"
		],
		"description": "Amexio Accordian Pane Transperent"
	},
	//Layout-border
	"amexio-border-layout": {
		"prefix": "amexio-border-layout",
		"body": [
			"<amexio-borderlayout>",
			"\t<amexio-borderlayout-item position=\"${1:position}\">",
			"\t\t${2:Content}",
			"\t</amexio-borderlayout-item>",
			"</amexio-borderlayout>"
		],
		"description": "Amexio Border Layout "
	},
	"amexio-border-layout-item": {
		"prefix": "amexio-border-layout-item",
		"body": [
			"\t<amexio-borderlayout-item position=\"${1:position}\">",
			"\t\t${2:Content}",
			"\t</amexio-borderlayout-item>"
		],
		"description": "Amexio Border Layout Item"
	},
	//Layout-column
	"amexio-row": {
		"prefix": "amexio-row",
		"body": [
			"<amexio-row>",
			"\t",
			"</amexio-row>"
		],
		"description": "Amexio Row"
	},
	"amexio-column": {
		"prefix": "amexio-column",
		"body": [
			"<amexio-column size =\"${1:size}\"",
			"\t\t\t\t[fit]=\"${2:false}\">",
			"\t",
			"</amexio-column>"
		],
		"description": "Amexio Column [size] :: 2:4:6:12"
	},
	//box
	"amexio-box": {
		"prefix": "amexio-box",
		"body": [
			"<amexio-box",
			"\tborder-color =\"${1:border-color}\"",
			"\tborder=\"${2:borderPosition}\"",
			"\t[border-dotted]=\"${3:false}\"",
			"\t[padding]=\"true\"",
			"\t[closable]=\"${4:false}\"",
			"\talign=\"${5:left}\">",
			"\t\t${6:Content}",
			"</amexio-box>"
		],
		"description": "Amexio Box "
	},
	"amexio-box-background": {
		"prefix": "amexio-box-background",
		"body": [
			"<amexio-box",
			"\tborder-color =\"${1:border-color}\"",
			"\tborder=\"${2:borderPosition}\"",
			"\t[border-dotted]=\"${3:false}\"",
			"\tbackground-color=\"${4:background-color}\"",
			"\talign=\"${5:left}\"",
			"\t[closable]=\"${6:false}\"",
			"\t[padding]=\"true\">",
			"\t\t${7:Content}",
			"</amexio-box>"
		],
		"description": "Amexio Box Background"
	},
	"amexio-box-background-height-width": {
		"prefix": "amexio-box-background-height-width",
		"body": [
			"<amexio-box",
			"\tborder-color =\"${1:border-color}\"",
			"\tborder=\"${2:borderPosition}\"",
			"\tborder-dotted=\"${3:false}\"",
			"\tbackground-color=\"${4:background-color}\"",
			"\tbox-height=\"${5:height}\"",
			"\tbox-width=\"${6:width}\"",
			"\talign=\"${7:left}\"",
			"\tclosable=\"${8:false}\"",
			"\tpadding=\"true\">",
			"\t\t${9:Content}",
			"</amexio-box>"
		],
		"description": "Amexio Box with background height and width "
	},
	//Layout-card
	"amexio-card": {
		"prefix": "amexio-card",
		"body": [
			"<amexio-card",
			"\t[header]=\"${1:false}\"",
			"\t[header-align]=\"'${2:left}'\"",
			"\t[footer]=\"${3:false}\"",
			"\t[footer-align]=\"'${4:left}'\"",
			"\t[show]=\"'${5:showVariable}'\"",
			"\theight=\"${6:height}\">",
			"\t\t<amexio-header>",
			"\t\t\t${7:header}",
			"\t\t</amexio-header>",
			"\t\t<amexio-body>",
			"\t\t\t${8:body}",
			"\t\t</amexio-body>",
			"\t\t<amexio-action>",
			"\t\t\t${9:footer}",
			"\t\t</amexio-action>",
			"</amexio-card>"
		],
		"description": "Amexio Card "
	},
	"amexio-card-with-height": {
		"prefix": "amexio-card-with-height",
		"body": [
			"<amexio-card",
			"\t[header]=\"${1:true}\"",
			"\t[header-align]=\"'${2:right}'\"",
			"\t[footer]=\"${3:false}\"",
			"\t[footer-align]=\"'${4:right}'\"",
			"\t[show]=\"'${5:showVariable}'\"",
			"\t[body-height]=\"'${6:height}'\"",
			"\t[min-height]=\"${7:height}\"",
			"\t[height]=\"${8:height}\">",
			"\t\t<amexio-body>",
			"\t\t\t${9:body}",
			"\t\t</amexio-body>",
			"</amexio-card>"
		],
		"description": "Amexio Card with Height"
	},
	"amexio-card-with-flag": {
		"prefix": "amexio-card-with-flag",
		"body": [
			"<amexio-card",
			"\t[header]=\"${1:true}\"",
			"\t[header-align]=\"'${2:right}'\"",
			"\t[footer]=\"${3:false}\"",
			"\t[footer-align]=\"'${4:right}'\"",
			"\t[show]=\"${5:flag}\"",
			"\t[body-height]=\"'${6:height}'\"",
			"\t[height]=\"${7:height}\">",
			"\t\t<amexio-body>",
			"\t\t\t${8:body}",
			"\t\t</amexio-body>",
			"</amexio-card>"
		],
		"description": "Amexio Card with Flag"
	},
	"amexio-card-image": {
		"prefix": "amexio-card-image",
		"body": [
			"<amexio-card",
			"\t[header]=\"${1:false}\"",
			"\t[header-align]=\"'${2:left}'\"",
			"\t[footer]=\"${3:false}\"",
			"\t[footer-align]=\"'${4:left}'\"",
			"\t[show]=\"${5:true}\"",
			"\t[body-height]=\"'${6:height}'\">",
			"\t\t<amexio-header>",
			"\t\t${7:header}",
			"\t\t</amexio-header>",
			"\t\t<amexio-body>",
			"\t\t\t<amexio-image path=\"${8:image}\">",
			"\t\t\t</amexio-image> ",
			"\t\t</amexio-body>",
			"</amexio-card>"
		],
		"description": "Amexio Card with image"
	},
	"amexio-layout-columns": {
		"prefix": "amexio-layout-columns",
		"body": [
			"<amexio-layout-columns",
			"\t[border]=\"${1:true}\"",
			"\t[fit]=\"${2:true}\"",
			"\t[alignment]=\"'${3:alignment}'\"",
			"\t[orientation]=\"'${4:vertical}'\">",
			"\t<amexio-layout-item",
			"\t\t[fit]=\"${2:true}\">",
			"\t</amexio-layout-item>",
			"</amexio-layout-columns>"
		],
		"description": "Amexio Layout Columns"
	},
	"amexio-layout-item": {
		"prefix": "amexio-layout-item",
		"body": [
			"<amexio-layout-item",
			"\t[fit]=\"${2:true}\">",
			"</amexio-layout-item>"
		],
		"description": "Amexio Layout Item"
	},
	"amexio-layout-grid":{
		"prefix": "amexio-layout-grid",
		"body": ["<amexio-layout-grid",
					"\t[layout]=\"'${1:layout}'\">",
					"\t<amexio-grid-item",
					"\t\t[name]=\"'${2:name}'\">",
				"\t</amexio-grid-item>",
				"</amexio-layout-grid>"	
				],
		"description":"Amexio Layout Grid"
	},
	"amexio-grid-item":{
		"prefix": "amexio-grid-item",
		"body": ["<amexio-grid-item",
					"\t[name]=\"'${1:name}'\">",
				"</amexio-grid-item>"	
				],
		"description": "Amexio Grid Item"
	},
	
	//Panel
	//Panel-carousel
	"amexio-carousel": {
		"prefix": "amexio-carousel",
		"body": [
			"<amexio-carousel [header]=\"'${1:Header}'\" [data]=\"${2:data}\">",
			"\t<ng-template let-img amexioTemplate=\"${3:item}\">",
			"\t\t${4:content}",
			"\t</ng-template>",
			"</amexio-carousel>"
		],
		"description": "Amexio Carousel"
	},
	"amexio-carousel-shuffle": {
		"prefix": "amexio-carousel-shuffle",
		"body": [
			"<amexio-carousel [header]=\"'${1:header}'\" [data]=\"${2:data}\" shuffle-interval=\"${3:interval}\">",
			"\t<ng-template let-img amexioTemplate=\"${4:item}\">",
			"\t\t${5:content}",
			"\t</ng-template>",
			"</amexio-carousel>"
		],
		"description": "Amexio Carousel with Shuffle Interval"
	},
	//Panels-Form
	"amexio-form": {
		"prefix": "amexio-form",
		"body": [
			"<amexio-form",
			"\t[header]=\"${1:false}\"",
			"\t[header-align]=\"'${2:left}'\"",
			"\t[footer]=\"${3:true}\"",
			"\t[footer-align]=\"'${4:right}'\"",
			"\t[min-height]=\"${5:height}\"",
			"\t[body-height]=\"'${6:height}'\"",
			"\t[form-name]=\"${7:name}\"",
			"\t[show-error]=\"${8:false}\"",
			"\t(showErrorMsg)=\"${9:functionName}(\\$event)\">",
			"\t\t<amexio-form-header>",
			"\t\t\t${10:header}",
			"\t\t</amexio-form-header>",
			"\t\t<amexio-form-body>",
			"\t\t\t${11:body}",
			"\t\t</amexio-form-body>",
			"\t\t<amexio-form-action>",
			"\t\t\t${12:footer}",
			"\t\t</amexio-form-action>",
			"</amexio-form>"
		],
		"description": "Amexio Form "
	},
	//Panels-fieldset
	"amexio-fieldset": {
		"prefix": "amexio-fieldset",
		"body": [
			"<amexio-fieldset [collapsible]=\"${1:false}\" [title]=\"${2:title}\">",
			"</amexio-fieldset>"
		],
		"description": "Amexio Fieldset"
	},
	"amexio-fieldset-collapsed": {
		"prefix": "amexio-fieldset-collapsed",
		"body": [
			"<amexio-fieldset [collapsible]=\"true\" [title]=\"${1:title}\">",
			"</amexio-fieldset>"
		],
		"description": "Amexio Fieldset "
	},
	//Panels-panel
	"amexio-panel": {
		"prefix": "amexio-panel",
		"body": [
			"<amexio-panel",
			"\t[header]=\"${1:true}\"",
			"\t[title]=\"'${2:title}'\"",
			"\t[height]=\"${3:height}\"",
			"\t[expanded]=\"${4:true}\"",
			"\t[border]=\"${5:true}\"",
			"\tcontent-align=\"'${6:left}'\"",
			"\t[collapsible]=\"${7:true}\"",
			"\t[context-menu]=\"${8:context-menu}\"",
			"\t(onClick)=\"${5:functionName}(\\$event)\">",
			"\t\t${6:content}",
			"</amexio-panel>"
		],
		"description": "Amexio Panel"
	},
	"amexio-panel-custom-header": {
		"prefix": "amexio-panel-custom-header",
		"body": [
			"<amexio-panel",
			"\t[header]=\"true\"",
			"\t[title]=\"'${1:title}'\"",
			"\t[height]=\"${2:height}\"",
			"\t[expanded]=\"true\"",
			"\t[border]=\"${5:border}\"",
			"\t[content-align]=\"${6:left}\"",
			"\t[collapsible]=\"${7:true}\"",
			"\t[context-menu]=\"${8:context-menu}\"",
			"\t(onClick)=\"${3:functionName}(\\$event)\">",
			"\t\t<amexio-header>",
			"\t\t\t${4:content}",
			"\t\t</amexio-header>",
			"</amexio-panel>"
		],
		"description": "Amexio Panel Custom Header"
	},
	"amexio-panel-no-header": {
		"prefix": "amexio-panel-no-header",
		"body": [
			"<amexio-panel",
			"\t[header]=\"false\"",
			"\t[title]=\"'${1:title}'\"",
			"\t[height]=\"${2:height}\"",
			"\t[expanded]=\"true\"",
			"\t[border]=\"${5:border}\"",
			"\t[content-align]=\"${6:left}\"",
			"\t[collapsible]=\"${7:true}\"",
			"\t[context-menu]=\"${8:context-menu}\"",
			"\t(onClick)=\"${3:functionName}(\\$event)\">",
			"\t\t${4:content}",
			"</amexio-panel>"
		],
		"description": "Amexio Panel without Header"
	},
	//Panels-steps
	"amexio-steps": {
		"prefix": "amexio-steps",
		"body": [
			"<amexio-steps [block]=\"false\" [icon]=\"false\" [index]=\"false\" [data]=\"'${1:data}'\">",
			"\t<amexio-step-block [label]=\"'${2:label}'\" [icon]=\"${3:icon}\" [active]=\"true\"  (onClick)=\"${4:functionName}(\\$event)\">",
			"\t</amexio-step-block>",
			"</amexio-steps>"
		],
		"description": "Amexio Steps"
	},
	"amexio-steps-block": {
		"prefix": "amexio-steps-block",
		"body": [
			"\t<amexio-step-block [label]=\"'${1:label}'\" [icon]=\"${2:icon}\" [active]=\"true\" (onClick)=\"${3:functionName}(\\$event)\" (getStepBlockData)=\"${4:functionName}(\\$event)\">",
			"\t</amexio-step-block>"
		],
		"description": "Amexio Steps Block"
	},
	"amexio-steps-block-with-icon": {
		"prefix": "amexio-step-block-with-icon",
		"body": [
			"<amexio-steps [block]=\"false\" [icon]=\"true\" (onClick)=\"${1:functionName}(\\$event)\">",
			"\t<amexio-step-block [label]=\"'${2:label}'\" [active]=\"false\" [icon]=\"${3:icon}\">",
			"\t</amexio-step-block>",
			"</amexio-steps>"
		],
		"description": "Amexio Steps Block with Icon"
	},
	"amexio-steps-clickable": {
		"prefix": "amexio-steps-clickable",
		"body": [
			"<amexio-steps [block]=\"true\" [index]=\"true\" (onClick)=\"${1:functionName}(\\$event)\">",
			"\t<amexio-step-block [label]=\"'${2:label}'\" [active]=\"true\">",
			"\t</amexio-step-block>",
			"</amexio-steps>"
		],
		"description": "Amexio Steps Clickable"
	},
	"amexio-steps-with-index": {
		"prefix": "amexio-steps-with-index",
		"body": [
			"<amexio-steps [index]=\"true\" (onClick)=\"${1:functionName}(\\$event)\">",
			"\t<amexio-step-block [label]=\"'${2:label}'\" [active]=\"true\">",
			"\t</amexio-step-block>",
			"</amexio-steps>"
		],
		"description": "Amexio Steps with Index"
	},
	//Panels-tab
	//Panels-tab-horizontal
	"amexio-tab-view": {
		"prefix": "amexio-tab-view",
		"body": [
			"<amexio-tab-view  [closable]=\"true\"",
			"\t[header-align]=\"'${1:left}'\"",
			"\t[action]=\"false\" [divide-header-equally]=\"false\"",
			"\t[tab-position]=\"${2:top}\" [header]=\"'${3:header}'\"",
			"\t[body-height]=\"${4:body-height}\"",
			"\t[context-menu]=\"'${5:context-menu}'\"", 
			"\t[default-context-menu]=\"false\">",
			"\t<amexio-tab title=\"${6:title}\"",
			"\t[active]=\"true\" [closable]=\"false\"",
			"\t[disabled]=\"false\" [icon]=\"${7:icon}\"",
			"\t[amexio-color]=\"'${8:color}'\"",
			"\t(onClick)=\"${9:functionName}(\\$event)\">",
			"\t\t${10:content}",
			"\t</amexio-tab>",
			"</amexio-tab-view>"
		],
		"description": "Amexio Tab View"
	},
	"amexio-tab": {
		"prefix": "amexio-tab",
		"body": [
			"\t<amexio-tab title=\"${1:title}\"",
			"\t[active]=\"false\" [closable]=\"false\"", 
			"\t[disabled]=\"false\"", 
			"\t[icon]=\"${2:icon}\"",
			"\t[amexio-color]=\"'${8:color}'\"",
			"(onClick)=\"${4:functionName}(\\$event)\">",
			"\t\t${3:content}",
			"\t</amexio-tab>"
		],
		"description": "Amexio Tab"
	},
	"amexio-tab-view-closable": {
		"prefix": "amexio-tab-view-closable",
		"body": [
			"<amexio-tab-view  [closable]=\"true\"",
			"\t[header-align]=\"'${1:left}'\"",
			"\t[action]=\"false\" [divide-header-equally]=\"false\"",
			"\t[tab-position]=\"${2:top}\" [header]=\"'${3:header}'\"",
			"\t[body-height]=\"${4:body-height}\"",
			"\t[context-menu]=\"'${5:context-menu}'\"", 
			"\t[default-context-menu]=\"false\">",
			"\t<amexio-tab title=\"${6:title}\"",
			"\t[active]=\"true\" [closable]=\"true\"",
			"\t[disabled]=\"false\" [icon]=\"${7:icon}\"",
			"\t[amexio-color]=\"'${8:color}'\"",
			"\t(onClick)=\"${9:functionName}(\\$event)\">",
			"\t\t${10:content}",
			"\t</amexio-tab>",
			"</amexio-tab-view>"
		],
		"description": "Amexio Closable Tab"
	},
	"amexio-tab-with-icon": {
		"prefix": "amexio-tab-with-icon",
		"body": [
			"<amexio-tab",
			"\ttitle=\"${1:title}\"",
			"\t[active]=\"false\"",
			"\t[icon]=\"'${2:icon}'\"",
			"\t(onClick)=\"${3:functionName}(\\$event)\">",
			"\t\t${4:content}",
			"</amexio-tab>"
		],
		"description": "Amexio Tab with Icon"
	},
	"amexio-tab-view-with-icon": {
		"prefix": "amexio-tab-with-icon",
		"body": [
			"<amexio-tab-view [closable]=\"false\">",
			"\t<amexio-tab",
			"\t\ttitle=\"${1:title}\"",
			"\t\t[active]=\"true\"",
			"\t\t[icon]=\"'${2:icon}'\"",
			"\t\t(onClick)=\"${3:functionName}(\\$event)\">",
			"\t\t\t${4:content}",
			"\t</amexio-tab>",
			"</amexio-tab-view>"
		],
		"description": "Amexio Tab with Icon"
	},
	"amexio-enhanced-tab-view": {
		"prefix": "amexio-enhanced-tab-view",
		"body": [
			"<amexio-tab-view",
			"\t[closable]=\"false\"",
			"\t[header]=\"${1:true}\"",
			"\t[header-align]=\"'${2:left}'\"",
			"\t[action]=\"${3:false}\"",
			"\t[divide-header-equally]=\"${4:false}\"",
			"\t[body-height]=\"${5:body-height}\"",
