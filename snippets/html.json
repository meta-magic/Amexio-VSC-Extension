    ],
    "description": "Amexio xSmall Button"
  },
  "amexio-block-button": {
    "prefix": "amexio-block-button",
    "body": [
      "<amexio-btn [label]=\"'${1:label}'\"",
      "\t [type]=\"${2:type}\"",
      "\t [tooltipMessage]=\"'${3:tooltipMessage}'\"",
      "\t [size]=\"'large'\"",
      "\t [block]=\"'true'\">",
      "</amexio-btn>"
    ],
    "description": "Amexio Block Button"
  },
  //Button Group Snippets 
  "amexio-button-group": {
    "prefix": "amexio-button-group",
    "body": [
      "<amexio-btn-group>",
      "\t <amexio-btn-group-action [label]=\"'${1:btnGroupLabel}'\">",
      "\t </amexio-btn-group-action>",
      "\t <amexio-btn-group-action [label]=\"'${2:label1}'\"",
      "\t [type]=\"'Default'\"> \n</amexio-btn-group-action>",
      "\t <amexio-btn-group-action [label]=\"'${3:label2}'\"",
      "\t [type]=\"'primary'\"> \n</amexio-btn-group-action> ",
      "\t <amexio-btn-group-action [label]=\"'${4:label3}'\"",
      "\t [type]=\"'success'\"> \n</amexio-btn-group-action>",
      "\t <amexio-btn-group-action [label]=\"'${5:label3}'\"",
      "\t [type]=\"'warning'\"> \n</amexio-btn-group-action>",
      "\t <amexio-btn-group-action [label]=\"'${6:label4}'\"",
      "\t [type]=\"'$danger'\">\n</amexio-btn-group-action>\n</amexio-btn-group>"
    ],
    "description": "Amexio Button Group"
  },
  "amexio-button-group-states": {
    "prefix": "amexio-button-group-states",
    "body": [
      "<amexio-btn-group>",
      "\t <amexio-btn-group-action [label]=\"'${1:label1}'\"  [type]=\"'primary'\"",
      "\t  [tooltipMessage]=\"'Disabled'\" [disabled]=\"'true'\">",
      "</amexio-btn-group-action>",
      "\t <amexio-btn-group-action [label]=\"'${2:label2}'\" [type]=\"'primary'\"",
      "\t [tooltipMessage]=\"'Enabled'\"></amexio-btn-group-action>",
      "</amexio-btn-group>"
    ],
    "description": "Amexio Button Group States"
  },
  "amexio-button-group-icons": {
    "prefix": "amexio-button-group-icon",
    "body": [
      "<amexio-btn-group>",
      "\t <amexio-btn-group-action [label]=\"'${1:label1}'\" \n\t [type]=\"'primary'\"",
      "\t [tooltipMessage]=\"'Next'\" icon=\"chevron-right\">",
      "\t </amexio-btn-group-action>",
      "<amexio-btn-group-action [label]=\"'${2:label2}'\"\n\t [type]=\"'success'\"",
      "\t [tooltipMessage]=\"'Save'\" icon=\"save\">",
      "</amexio-btn-group-action>",
      "<amexio-btn-group-action [label]=\"'${3:label3}'\"\n\t [type]=\"'danger'\"",
      "\t [tooltipMessage]=\"'Delete'\" icon=\"trash\" >\n\t</amexio-btn-group-action>",
      "</amexio-btn-group>"
    ],
    "description": "Amexio Button Group Icons"
  },
  "amexio-size-button-group": {
    "prefix": "amexio-button-group-size",
    "body": [
      "<amexio-btn-group [size]=\"'${1:size}'\">",
      "\t <amexio-btn-group-action [label]=\"'${2:label1}'\" [type]=\"'primary'\"></amexio-btn-group-action>",
      "\t <amexio-btn-group-action [label]=\"'${3:label2}'\" [type]=\"'primary'\"  ></amexio-btn-group-action>",
      "\t <amexio-btn-group-action [label]=\"'${4:label3}'\" [type]=\"'primary'\"  ></amexio-btn-group-action>",
      "</amexio-btn-group>"
    ],
    "description": "Amexio Button Group Size"
  },
  //Button Group with Dropdown
  "amexio-button-dropdown": {
    "prefix": "amexio-button-dropdown",
    "body": [
      " <amexio-btn-dropdown [label]=\"'${1:btnDropDownLabel}'\"",
      "\t [type]=\"'${2:type}'\" [size]=\"'${3:size}'\">",
      "\t <amexio-btn-dropdown-item [label]=\"'${4:lblLink1}'\"",
