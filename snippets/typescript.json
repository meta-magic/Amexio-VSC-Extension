            "",
            "toggleBasicWindow(data: any) {",
            "\tif (data == 'material') {",
            "\t\tthis.showBasicWindowMaterial = !this.showBasicWindowMaterial;",
            "}",
            "\telse if (data == 'nonmaterial') {",
            "\t\tthis.showBasicWindowNonMaterial = !this.showBasicWindowNonMaterial;",
            "",
            "}",
            "\telse {",
            "\t\tthis.showBasicWindowNonMateialToolbar = !this.showBasicWindowNonMateialToolbar;",
            "\t\t}",
            "\t}",
