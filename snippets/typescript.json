            "\t\tthis.showMaxWindowMaterial = !this.showMaxWindowMaterial;",
            "\t}",
            "\telse {",
            "\t\tthis.showMaxWindowNonMaterial = !this.showMaxWindowNonMaterial;",
            "\t\t}",
            "\t}",
            "",            
            "toggleClosable(data: any) {",
